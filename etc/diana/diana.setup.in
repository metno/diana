OPDATA    = $(share_diana_dir)/demodata
MAPDIR    = $(share_dir)/diana/maps
THREDDS_MET_NO_DODSC = https://thredds.met.no/thredds/dodsC

<BASIC>
fontpath=$(share_diana_dir)/fonts
docpath=$(share_dir)/doc/diana-@PVERSION@
