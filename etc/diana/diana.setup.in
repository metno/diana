ff_normal     = normal(x_wind_ml, y_wind_ml)
ff_tangential = tangential(x_wind_ml, y_wind_ml)
sqrt_tke      = sqrt(turbulent_kinetic_energy_ml)
x_wind        =identity(x_wind_ml)
y_wind        =identity(y_wind_ml)
q             =identity(specific_humidity_ml)
w             =identity(upward_air_velocity_ml)
omega         =identity(omega_ml)

tk            = identity(air_temperature_pl)
tk            = tk_from_th(air_potential_temperature_pl)
th            = th_from_tk(air_temperature_pl)
th            = identity(air_potential_temperature_pl)
the           = the_from_tk_q(air_temperature_pl,specific_humidity_pl)
the           = the_from_th_q(air_potential_temperature_pl,specific_humidity_pl)
rh            = rh_from_tk_q(air_temperature_pl,specific_humidity_pl)
rh            = rh_from_th_q(air_potential_temperature_pl,specific_humidity_pl)
rh            = identity(relative_humidity_pl)
ff_total_ms   = total(x_wind_pl, y_wind_pl)
ff_normal     = normal(x_wind_pl, y_wind_pl)
ff_tangential = tangential(x_wind_pl, y_wind_pl)
sqrt_tke      = sqrt(turbulent_kinetic_energy_pl)
x_wind        =identity(x_wind_pl)
y_wind        =identity(y_wind_pl)
q             =identity(specific_humidity_pl)
w             =identity(upward_air_velocity_pl)
